{
   "loadco1":{
      "process_id":"load_collection",
      "arguments":{
         "id":"sentinel-2-l1c",
         "spatial_extent":null,
         "temporal_extent":[
            "2019-08-01",
            "2019-08-18"
         ],
         "bands":[
            "B04",
            "B08"
         ]
      }
   },
   "ndvi1":{
      "process_id":"reduce_dimension",
      "arguments":{
         "data":{
            "from_node":"loadco1"
         },
         "reducer":{
            "process_graph": {
               "2":{
                  "process_id":"mean",
                  "arguments":{
                     "data":{
                        "from_parameter":"data"
                     }
                  },
                  "result":true
               }
            }
         },
         "dimension":"bands"
      },
      "description":"Compute the NDVI: (NIR - RED) / (NIR + RED)"
   },
   "meanndvi1":{
      "process_id":"reduce_dimension",
      "arguments":{
         "data":{
            "from_node":"ndvi1"
         },
         "reducer":{
            "process_graph":{
               "2":{
                  "process_id":"mean",
                  "arguments":{
                     "data":{
                        "from_parameter":"data"
                     }
                  },
                  "result":true
               }
            }
         },
         "dimension":"t"
      }
   },
   "result1":{
      "process_id":"save_result",
      "arguments":{
         "data":{
            "from_node":"meanndvi1"
         },
         "format":"jpeg"
      },
      "result":true
   }
}